{
  "name": "react-polyfill",
  "description": "A builde of shims and polyfills to support React and HTML5/CSS3 for legacy browsers.",
  "version": "0.0.1",
  "author": "Minkyu Cho <mrnoname@naver.com>",
  "license": "MIT",
  "homepage": "https://github.com/Pitzcarraldo/react-polyfill",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist"
  ],
  "npmName": "react-polyfill",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pitzcarraldo/react-polyfill.git"
  },
  "bugs": {
    "url": "https://github.com/Pitzcarraldo/react-polyfill/issues"
  },
  "keywords": [
    "react-polyfill"
  ],
  "scripts": {
    "clean": "rimraf dist modernizr.js",
    "modernizr": "modernizr -c node_modules/modernizr/lib/config-all.json",
    "build:umd": "webpack --config webpack/webpack.config.development.js",
    "build:umd:min": "webpack --config webpack/webpack.config.production.js",
    "build": "npm run modernizr && npm run build:umd && npm run build:umd:min",
    "preversion": "npm run clean",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "compose-dataset-shim": "^1.0.0",
    "console-polyfill": "^0.2.2",
    "es3ify-loader": "^0.1.0",
    "es5-shim": "^4.5.5",
    "eventsource-polyfill": "^0.9.6",
    "karma": "^0.13.21",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "modernizr": "^3.3.1",
    "phantomjs": "^1.9.19",
    "phantomjs-prebuilt": "^2.1.4",
    "respond.js": "^1.4.2",
    "rimraf": "^2.3.4",
    "script-loader": "^0.6.1",
    "webpack": "<=1.12.12"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}